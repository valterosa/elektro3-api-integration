{
  "name": "elektro3-api-integration",
  "version": "1.0.0",
  "description": "Shopify app for Elektro3 API integration",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "prisma generate && node prisma-setup.js && node fix-shopify-json-import.js && remix vite:build",
    "vercel-build": "prisma generate && node vercel-db-setup.js && node fix-shopify-json-import.js && remix vite:build",
    "deploy:vercel": "node deploy-to-vercel.js",
    "dev": "set NODE_ENV=development&& node server.mjs",
    "dev:local": "node dev-server.js",
    "dev:express": "NODE_ENV=development node express-server.js",
    "dev:win": "set NODE_ENV=development&& node server.mjs",
    "dev:shopify": "shopify app dev",
    "dev:otimizado": "node local-server.js",
    "dev:build-serve": "node build-serve.js",
    "dev:https": "node https-local-server.js",
    "dev:tunnel": "node tunnel-server.js",
    "dev:remix": "remix vite:dev",
    "dev:opt": "node optimized-dev-server.js",
    "tunnel": "node https-tunnel.js",
    "bypass-tunnel": "node bypass-tunnel.js",
    "start": "NODE_ENV=production node server.mjs",
    "start:win": "set NODE_ENV=production&& node server.mjs",
    "shopify:link": "shopify app config link",
    "shopify:generate": "shopify app generate",
    "shopify:deploy": "shopify app deploy",
    "shopify:config": "shopify app config use",
    "shopify:env": "shopify app env",
    "prisma:setup": "prisma generate && prisma migrate deploy",
    "docker-start": "npm run prisma:setup && npm run start",
    "verify-build": "node verify-build.js",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "clean": "rimraf build node_modules/.cache",
    "fix-imports": "node fix-imports.js",
    "fix-json-import": "node fix-shopify-json-import.js",
    "check-env": "node check-environment.js",
    "test-connection": "node test-connection.js",
    "prepare-local": "npm run check-env && npm run fix-imports && npm run fix-json-import",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "@remix-run/dev": "^2.16.1",
    "@remix-run/express": "^2.16.1",
    "@remix-run/fs-routes": "^2.16.1",
    "@remix-run/node": "^2.16.1",
    "@remix-run/react": "^2.16.1",
    "@remix-run/route-config": "^2.16.1",
    "@remix-run/serve": "^2.16.1",
    "@shopify/app-bridge-react": "^4.1.6",
    "@shopify/network": "3.3.0",
    "@shopify/polaris": "^12.0.0",
    "@shopify/shopify-api": "^11.12.0",
    "@shopify/shopify-app-remix": "^3.7.0",
    "@shopify/shopify-app-session-storage-prisma": "^6.0.0",
    "@vercel/node": "^3.0.25",
    "@vercel/remix": "2.15.3",
    "axios": "^1.9.0",
    "compression": "1.8.0",
    "dotenv": "16.5.0",
    "express": "^4.18.2",
    "isbot": "^5.1.0",
    "morgan": "1.10.0",
    "prisma": "^6.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^6.2.2",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "devDependencies": {
    "@remix-run/eslint-config": "^2.16.1",
    "@shopify/api-codegen-preset": "^1.1.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.2.0",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^10.0.1",
    "http-proxy-middleware": "3.0.5",
    "open": "10.1.2",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "selfsigned": "2.4.1"
  },
  "workspaces": [
    "extensions/*"
  ],
  "engines": {
    "node": "^18.20 || ^20.10 || >=21.0.0"
  },
  "trustedDependencies": [
    "@shopify/plugin-cloudflare"
  ],
  "resolutions": {
    "@graphql-tools/url-loader": "8.0.16",
    "@graphql-codegen/client-preset": "4.7.0",
    "@graphql-codegen/typescript-operations": "4.5.0",
    "minimatch": "9.0.5"
  },
  "overrides": {
    "@graphql-tools/url-loader": "8.0.16",
    "@graphql-codegen/client-preset": "4.7.0",
    "@graphql-codegen/typescript-operations": "4.5.0",
    "minimatch": "9.0.5"
  },
  "author": "Valter"
}
