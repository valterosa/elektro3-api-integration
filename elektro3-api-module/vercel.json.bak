{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "rewrites": [{ "source": "/(.*)", "destination": "/api/$1" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/$1",
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/api/$1"
    }
  ]
}
