{
  "version": 2,
  "buildCommand": "cd elektro3-api-module && npm install --include=dev && npm run build",
  "outputDirectory": "elektro3-api-module/build",
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
